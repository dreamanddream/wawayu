@import "../../template/evaluation/evaluation-template.wxss";
.evalute{
  padding-bottom:30rpx;
  border-bottom:1px solid #e8e8e8;
}
.label-wrap{
  width:90%;
  margin:0 auto;
}
.label-wrap .label{
  padding:15rpx 25rpx;
  border:1px solid #e8e8e8;
  font-size:14px;
  float:left;
  margin-right:20rpx;
  margin-bottom:20rpx;
  text-align: center;
  border-radius: 20px;
}
.label-wrap .label.active{
  background: #604687;
  color:#fff;
  border:none;
}
